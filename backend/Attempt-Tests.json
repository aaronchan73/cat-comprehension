{ 
    "testCases": [
        {
            "id": 1, 
            "name": "twoSum",
            "testCases": [
                {
                    "test": "basic case", 
                    "input": "[2, 7, 11, 15], 9", 
                    "expectedOutput": "[0,1]" 
                }, 
                {
                    "test": "duplicate numbers", 
                    "input": "[3, 3], 6", 
                    "expectedOutput": "[0, 1]" 
                }, 
                { 
                    "test": "0s", 
                    "input": "[0, 4, 3, 0], 0",
                    "expectedOutput": "[0, 3]" 
                },
                {
                    "test": "no solution",
                    "input": "[1, 2, 3], 8", 
                    "expectedOutput": "[]"
                },
                {
                    "test": "negative numbers",
                    "input": "[-1, -2, -3, -4, -5], -8",
                    "expectedOutput": "[2, 4]"  
                },
                {
                    "test": "positive and negative numbers", 
                    "input": "[1, -2, 3, -4, 5], 1", 
                    "expectedOutput": "[0, 3]"
                },
                {
                    "test": "large numbers", 
                    "input": "[1000000, 999999], 1", 
                    "expectedOutput": "[0, 1]"
                }, 
                {
                    "test": "1 element in list", 
                    "input": "[1], 1", 
                    "expectedOutput": "[]"  
                },
                {
                    "test": "empty list", 
                    "input": "[], 1",
                    "expectedOutput": "[]"
                },
                {
                    "test": "more than 1 solution", 
                    "input": "[1, 1, 3, 3], 4", 
                    "expectedOutput": "[1, 2]"
                }
            ]
        }, 
        {
            "id": 2, 
            "name": "add", 
            "testCases": [
                {
                    "test": "basic case", 
                    "input": "2, 3",
                    "expectedOutput": "5" 
                },
                {
                    "test": "0s", 
                    "input": "0, 0", 
                    "expectedOutput": "0"
                }, 
                {
                    "test": "negative numbers", 
                    "input": "-1, -1", 
                    "expectedOutput": "-2"
                }, 
                {
                    "test": "decimals", 
                    "input": "1.5, 2,5", 
                    "expectedOutput": "4.0"
                }, 
                {
                    "test": "large numbers", 
                    "input": "1000000, 1000000",
                    "expectedOutput": "2000000"
                },
                {
                    "test": "positive and negative numbers",
                    "input": "-2, 2", 
                    "expectedOutput": "0"
                }, 
                {
                    "test": "infinity", 
                    "input": "Infinity, -Infinity", 
                    "expectedOutput": "NaN"
                }
            ]
        }
    ]
}